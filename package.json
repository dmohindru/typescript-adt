{
  "name": "@dmohindru/typescript-adt",
  "version": "0.0.1-beta",
  "description": "Common Abstract datatype implementation in Typescript",
  "scripts": {
    "publish": "npm dist && npm publish --access public",
    "dist": "npm run test:coverage && webpack",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" \"test/**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "keywords": [
    "data",
    "structures",
    "typescript"
  ],
  "author": "Dhruv Mohindru",
  "license": "GNU",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "source-map-loader": "^5.0.0",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
