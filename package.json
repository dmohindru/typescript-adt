{
  "name": "@dmohindru/typescript-adt",
  "version": "0.0.1-TEST3",
  "description": "Common Abstract datatype implementation in Typescript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./list": {
      "import": "./dist/list/index.mjs",
      "require": "./dist/list/index.js"
    },
    "./interfaces": {
      "import": "./dist/interfaces/index.mjs",
      "require": "./dist/interfaces/index.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "deploy": "npm run dist && npm publish --access public",
    "dist": "npm run test:coverage && npm run build",
    "build": "rm -rf dist coverage && tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\" \"test/**/*.{ts,tsx,js,jsx,json,css,scss,md}\""
  },
  "keywords": [
    "data",
    "structures",
    "typescript"
  ],
  "author": "Dhruv Mohindru",
  "license": "GNU",
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "tsup": "^8.2.3",
    "typescript": "^5.5.3"
  }
}
